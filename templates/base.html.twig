<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>{% block title %}{{ siteName }} {{ siteSlogan }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="description" content=""/>
    <meta name="author" content=""/>
    <meta charset="UTF-8">

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700" rel="stylesheet">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('build/images/ico/apple-touch-icon-144-precomposed.png') }}" />
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('build/images/ico/apple-touch-icon-114-precomposed.png') }}" />
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('build/images/ico/apple-touch-icon-72-precomposed.png') }}" />
        <link rel="apple-touch-icon-precomposed" href="{{ asset('build/images/ico/apple-touch-icon-57-precomposed.png') }}" />
        <link rel="shortcut icon" href="{{ asset('build/images/ico/favicon.png') }}" />
    {% endblock %}
</head>
<body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('jquery') }}
        {{ encore_entry_script_tags('cond') }}
        {{ encore_entry_script_tags('easing') }}
        {{ encore_entry_script_tags('bootstrap') }}
        {{ encore_entry_script_tags('jcarousel') }}
        {{ encore_entry_script_tags('fancybox') }}
        {{ encore_entry_script_tags('google') }}
        {{ encore_entry_script_tags('portfolio') }}
        {{ encore_entry_script_tags('slider') }}
        {{ encore_entry_script_tags('animate') }}
        {{ encore_entry_script_tags('custom') }}
    {% endblock %}
</body>
</html>